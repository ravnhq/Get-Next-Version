{
  "name": "compare-and-bump-action",
  "version": "1.0.0",
  "private": true,
  "description": "Compare version numbers to package.json versions and bump",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravnhq/compare-and-bump-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "RavnHQ",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "git-semver-tags": "^4.1.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/git-semver-tags": "^4.1.1",
    "@types/node": "^16.10.5",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.34.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
